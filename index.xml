                type: &#39;value&#39;,
                name: &#39;Energy recovery retention (%)&#39;,
                nameGap: 20,
                min: 60,  // 设置最小值为 60%
                max: 100,
                axisLabel: {
                    formatter: &#39;{value} %&#39;
                }
            },
            dataZoom: [
                {
                    type: &#39;slider&#39;,
                    start: 0,
                    end: 100
                },
                {
                    type: &#39;inside&#39;
                }
            ],
            series: seriesData.map(item =&gt; ({
                name: item.name,
                type: &#39;line&#39;,
                data: item.data,
                smooth: true,
                symbol: &#39;circle&#39;,
                symbolSize: 8,
                lineStyle: {
                    width: 2
                },
                connectNulls: true  // 处理空数据，使曲线连续
            }))
        };
        // 自适应窗口大小变化
        // window.addEventListener(&#39;resize&#39;, function() {
        //     myChart.resize();
        // });
    // 使用 setOption 方法来设置图表选项
    myChart5.setOption(option5);
&lt;/script&gt;</description>
